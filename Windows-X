number = Value
end)

ss:Toggle("Chest Farm",function(t)
    if number then
getgenv().Chest = true
while Chest do task.wait()

for _,v in pairs(game:GetService("Workspace").Map.Islands:GetDescendants()) do
   if v:FindFirstChild("ClickDetectorPart") and v:IsA"Model" then
local Time = number
